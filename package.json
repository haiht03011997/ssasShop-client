    "react-helmet-async": "^2.0.5",
    "react-hook-form": "7.54.2",
    "react-jhipster": "0.25.3",
    "react-loadable": "5.5.0",
    "react-redux": "9.2.0",
    "react-redux-loading-bar": "5.0.8",
    "react-router": "7.1.5",
